<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" href="./images/favicon.ico">

    @*@Styles.Render("~/css")

        @Scripts.Render("~/bundles/modernizr")*@

    @Styles.Render("~/vendor/bootstrap/css/bootstrap.min.css")
    <link href="vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <!-- Custom fonts for this template -->
    @Styles.Render("~/vendor/font-awesome/css/font-awesome.min.css")
    <link href="vendor/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <!-- Plugin CSS -->
    @Styles.Render("~/vendor/datatables/dataTables.bootstrap4.css")
    <link href="vendor/datatables/dataTables.bootstrap4.css" rel="stylesheet">
    <!-- Custom styles for this template -->
    @Styles.Render("~/css/sb-admin.css")
    <link href="css/sb-admin.css" rel="stylesheet">

    <title>Monitoria Fatec  - @ViewBag.Title </title>

    <!-- Custom styles for this template -->

</head>
<body>

    <div>
        @RenderBody()
      
        <footer>
            <p>&copy; @DateTime.Now.Year - Monitoria FATEC</p>
        </footer>
    </div>

    @*@Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")*@



    @Scripts.Render("~/vendor/jquery/jquery.min.js")
    <script src="vendor/tether/tether.min.js"></script>
    @Scripts.Render("~/vendor/tether/tether.min.js")
    <script src="vendor/bootstrap/js/bootstrap.min.js"></script>
    @Scripts.Render("~/vendor/bootstrap/js/bootstrap.min.js")
    <!-- Plugin JavaScript -->
    <script src="vendor/chart.js/Chart.min.js"></script>
    @Scripts.Render("~/vendor/chart.js/Chart.min.js")
    <script src="vendor/datatables/jquery.dataTables.min.js"></script>
    @Scripts.Render("~/vendor/datatables/jquery.dataTables.min.js")
    <script src="vendor/datatables/dataTables.bootstrap4.min.js"></script>
    @Scripts.Render("~/vendor/datatables/dataTables.bootstrap4.min.js")
    <!-- Custom scripts for this template -->
    @Scripts.Render("~/js/sb-admin.min.js")
    <script src="js/sb-admin.min.js"></script>


    @RenderSection("scripts", required: false)
</body>
</html>
